pragma solidity 0.8.17;

import "@openzeppelin/contracts/token/ERC20/ERC20.sol";

contract WinToken is ERC20 {
    constructor(uint256 totalSupply) ERC20("WIN", "WIN") {
        _mint(msg.sender, totalSupply * (10 ** decimals()));
    }
}
